{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": "off",
        "no-unused-vars": "warn",
        "linebreak-style": "off",
        "arrow-body-style":"off",
        "react/function-component-definition": "off",
        "consistent-return":"off",
        "react/react-in-jsx-scope": "off"
    }
}
